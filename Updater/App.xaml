<Application x:Class="Updater.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\Button.xaml"/>
                <ResourceDictionary Source="Styles\ProgressBar.xaml"/>
                <ResourceDictionary Source="Styles\RadioButton.xaml"/>
                <ResourceDictionary Source="Styles\RadioListBox.xaml"/>
                <ResourceDictionary Source="Styles\StartButton.xaml"/>
                <ResourceDictionary Source="Styles\TextBlock.xaml"/>
                <ResourceDictionary Source="Styles\TitleButton.xaml"/>
                <ResourceDictionary Source="Styles\Window.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
